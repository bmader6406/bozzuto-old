<% render_meta(@page) %>

<% @page.ancestors.each do |page| %>
  <% if @section.service? %>
    <% breadcrumb_item(link_to page.title, services_page_path(@section, page)) %>
  <% else %>
    <% breadcrumb_item(link_to page.title, page_path(@section, page)) %>
  <% end %>
<% end %>
<% breadcrumb_item(@page.title) %>

<% breadcrumb_title(@page.title) %>

<%= @page.body.try(:html_safe) %>


<% if request.path =~ %r{^/apartments(|/overview/?)?$} %>
  <% neighborhoods_assets %>
<% end %>


<% if params[:section] == 'apartments' %>
  <% if @page.slug == 'overview' %>
    <!-- Apartments Landing Page -->
    <iframe src="//img-cdn.mediaplex.com/0/16798/universal.html?page_name=bozzuto_apts_lp&Bozzuto_Apts_LP=1&mpuid=" HEIGHT="1" WIDTH="1" FRAMEBORDER="0"></iframe>
  <% end %>
<% end %>
